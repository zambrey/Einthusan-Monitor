{
	"manifest_version": 2,

	"name": "Einthusan Monitor",
	"description": "This extension monitors www.einthusan.com for new HD movies available.",
	"version": "0.0",

  "icons":
  {
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
	"browser_action": 
	 {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
    },

   "background": 
   {
    "scripts": ["background.js"]
    },

    "options_page":"options.html",

    "permissions": ["tabs",
    "cookies",
    "storage",
    "https://www.einthusan.com/", 
    "http://www.einthusan.com/",
    "https://einthusan.tv/"]
  }